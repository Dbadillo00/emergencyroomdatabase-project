--Query the Data
--Which patients have a temperature of 100 degrees??
SELECT * FROM PatientData WHERE temperature= ‘100’;

--Query the Data
--Which patients are in the emergency room for a heart attack??
SELECT * FROM PatientData WHERE reason_for_visit = ‘heart attack’;

--Query the Data
--Which patients have a shellfish allergy??
SELECT * FROM PatientData WHERE allergies = ‘shellfish’;
