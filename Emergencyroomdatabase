-- Create database for the ER
CREATE DATABASE EmergencyRoomDatabase;

-- Use database
USE EmergencyRoomDatabase;

-- Create Table with columns
create table PatientData (
	patient_id INT,
	date_time DATE,
	first_name VARCHAR(50),
	last_name VARCHAR(50),
	age INT,
	insurance_ID VARCHAR(50),
	systolic_pressure INT,
	diastolic_pressure INT,
	heart_rate INT,
	temperature DECIMAL(6,3),
	allergies  VARCHAR(10),
	reason_for_visit VARCHAR(20)

);

--View the created Columns
SHOW COlUMNS FROM PatientData;


